{
  "name": "salon-booking-server",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
  "build": "tsc -p tsconfig.json",
  "type-check": "tsc --noEmit -p tsconfig.json",
    "build:prod": "npm --prefix ../client run build && npx prisma generate && npx prisma generate --schema=prisma/business.prisma && npm run build",
    "start": "node dist/index.js",
    "prisma": "prisma",
    "prisma:generate:business": "prisma generate --schema prisma/business.prisma",
    "seed": "ts-node --transpile-only src/seed.ts",
  "postinstall": "prisma generate && npm run prisma:generate:business"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.30",
    "prisma": "^5.20.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
